//Ler um vetor C de 10 elementos inteiros, trocar todos os valores negativos do vetor C por 0. Escrever o vetor C modificado.

#include <stdio.h>

int main() {
    int vetorC[10];
    printf("Elementos do vetor C: \n");
    for (int i=0; i<10; i++){
        printf("[%d] ", i);
        scanf("%d", &vetorC[i]);

        if (vetorC[i]<0){
            vetorC[i] = 0;
        }
    }
    for (int i=0; i<10; i++){
        printf("[%d] ", vetorC[i]);
    }
    return 0;
}